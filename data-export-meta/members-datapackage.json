                    },
                    {
                        "name": "addresses",
                        "type": "string",
                        "format": "default",
                        "rdfType": "https://schema.org/ItemList"
                    },
                    {
                        "name": "postal_codes",
                        "type": "string",
                        "format": "default",
                        "rdfType": "https://schema.org/ItemList",
                        "constraints": {
                            "pattern": "(\\d{5}|[A-Z \\d]+|)(;(\\d{5}|[A-Z \\d]+|))*"
